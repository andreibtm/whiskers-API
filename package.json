{
  "name": "whiskers-api",
  "version": "1.0.0",
  "description": "Whiskers API backend",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/server.js",
    "lint": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --name init",
    "db:push": "prisma db push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andreibtm/whiskers-API.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/andreibtm/whiskers-API/issues"
  },
  "homepage": "https://github.com/andreibtm/whiskers-API#readme",
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/supertest": "^6.0.3",
    "@types/pg": "^8.16.0",
    "prisma": "^7.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^2.1.4"
  },
  "dependencies": {
    "@fastify/cors": "^8.4.2",
    "@fastify/jwt": "^7.2.3",
    "@fastify/rate-limit": "^8.0.3",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^1.9.1",
    "@fastify/sensible": "^5.6.0",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^17.2.3",
    "fastify": "^4.28.1",
    "fastify-plugin": "^5.0.1",
    "fastify-type-provider-zod": "^2.1.0",
    "http-status-codes": "^2.3.0",
    "pg": "^8.16.3",
    "zod": "^3.23.8",
    "supertest": "^7.0.0"
  }
}
